{
  "expo": {
    "name": "BwebEscan",
    "slug": "eventPartner_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor":"#000",
    "newArchEnabled": true,
    "fonts": [
      {
        "asset": "./assets/fonts/Poppins-Regular.ttf",
        "family": "Poppins-Regular"
      },
      {
        "asset": "./assets/fonts/Poppins-Bold.ttf",
        "family": "Poppins-Bold"
      },
      {
        "asset": "./assets/fonts/Poppins-Medium.ttf",
        "family": "Poppins-Medium"
      },
      {
        "asset": "./assets/fonts/Poppins-SemiBold.ttf",
        "family": "Poppins-SemiBold"
      }
    ],
    "splash": {
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wa2goose.ExpoQR2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/BwebPartner.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.wa2goose.ExpoQR2",
      "config": {
        "usesPermissionsFlags": {
          "camera": true
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow this app to access the camera for scanning QR codes.",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-barcode-scanner"
    ],
    "extra": {
      "eas": {
        "projectId": "ed00d720-a37c-41de-b4d4-2feced7a38f0"
      }
    }
  }
}
